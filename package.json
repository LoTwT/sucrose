{
  "name": "@ayingott/sucrose",
  "version": "0.0.34",
  "description": "sucrose",
  "type": "module",
  "packageManager": "pnpm@8.7.1",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/sucrose#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/sucrose.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/sucrose/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.5.2",
    "@ayingott/prettier-config": "^0.1.0",
    "@types/node": "^20.5.9",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.7.1",
    "prettier": "^3.0.3",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "vitest": "^0.34.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": "@ayingott/prettier-config"
}
