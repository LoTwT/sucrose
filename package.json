{
  "name": "@ayingott/sucrose",
  "version": "0.0.9",
  "packageManager": "pnpm@8.6.12",
  "description": "sucrose",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/sucrose#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/sucrose.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/sucrose/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I",
    "a": "bumpp"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.2.7",
    "@types/node": "^20.4.9",
    "bumpp": "^9.1.1",
    "changelogen": "^0.5.4",
    "eslint": "^8.46.0",
    "lint-staged": "^13.2.3",
    "pnpm": "^8.6.12",
    "prettier": "^3.0.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "vitest": "^0.34.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@ayingott/eslint-config"
  }
}
